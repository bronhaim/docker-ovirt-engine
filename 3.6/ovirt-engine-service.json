{
  "kind": "Service",
  "apiVersion": "v1",
  "metadata": {
    "name": "ovirt-engine"
  },
  "spec": {
    "selector": {
      "app": "ovirt-engine"
    },
    "ports": [
      {
        "protocol": "TCP",
        "port": 80,
        "targetPort": "engine"
      },
      {
        "protocol": "TCP",
        "port": 443,
        "targetPort": "engine-tls"
      }
    ]
  }
}
